<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#26fdba30" />
            <corners android:radius="100dp" />
        </shape>
    </item>


    <item>
        <inset android:inset="6dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#73fdba30" />
            <corners android:radius="100dp" />
        </shape>
        </inset>
    </item>


    <item>
        <inset android:inset="6dp">
            <shape android:shape="rectangle">
                <stroke
                    android:width="4dp"
                    android:color="#99fdba30" />
                <corners android:radius="100dp" />
            </shape>
        </inset>
    </item>
    <item>
        <inset android:inset="6dp">
            <shape android:shape="rectangle">
                <solid  android:color="#FFfdba30" />
                <stroke
                    android:width="4dp"
                    android:color="#FFfdba30" />
                <corners android:radius="100dp" />
            </shape>
        </inset>
    </item>



</layer-list>
